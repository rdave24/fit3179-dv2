{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 400,
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/rdave24/rdave24.github.io/main/data/formula_one.fastest_times.json"
  },
  "mark": "line",
  "encoding": {
    "x": {"field": "races.year", "type": "quantitative", "title": "Year"},
    "y": {"field": "races.fastestLapTime", "type": "temporal", "title": "Fastest Lap Time"},
    "color": {"field": "track", "type": "nominal", "title": "Track"}
  }
}
